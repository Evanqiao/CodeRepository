#include <iostream> 

class Test
{
public:	
	Test();
	Test(int a[]);
	void print() const; 
private:
	static const int Num = 4;
	int a[Num];

};

int main()
{
	Test test;
	test.print();
	return 0;
}

Test::Test(int a[])
{
	a[0] = 1;
	a[1] = 2;
	a[2] = 3;
	a[3] = 4;
}
void Test::print() const
{
	using std::cout;
	using std::endl;
	for(int i = 0; i < 4; i++)	
		cout << a[i] << "	";
	cout << endl;
}
